{
    "compilerOptions": {
      "target": "es6",
      "module": "CommonJS",
      "typeRoots": ["./node_modules/@types", "./src/@types"],
      "outDir": "./dist",
      "strict": true,
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }